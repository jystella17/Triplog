<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.travel.route.repository.ImageRepository">
    <insert id="saveImage" parameterType="com.example.travel.route.entity.Image" useGeneratedKeys="true" keyProperty="img_id">
        insert into image(img_name, img_key, img_url)
        values(#{img_name}, #{img_key}, #{img_url})
    </insert>
</mapper>